{
  "name": "template-plop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-dev": "run-p type-check build-only --mode build-dev",
    "build-prod": "run-p type-check build-only --mode build-prod",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "g": "plop --plopfile generators/index.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/components": "^10.4.0",
    "@vueuse/core": "^10.4.0",
    "ant-design-vue": "4.0.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.2.0",
    "unplugin-vue-components": "^0.25.1",
    "use-scrollbars": "^0.4.8",
    "vue": "^3.3.4",
    "vue-i18n": "9.3.0-beta.26",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-html-env": "^1.2.8",
    "vite-plugin-mkcert": "^1.16.0",
    "vue-tsc": "^1.8.6"
  }
}
